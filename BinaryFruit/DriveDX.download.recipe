<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest release of DriveDX SMART monitoring and reporting tool.</string>
    <key>Identifier</key>
    <string>com.github.Ubertec.download.DriveDX</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>DriveDX</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>http://binaryfruit.com/download/drivedx/mac/1?ref=binaryfruit.com&amp;place=drivedx.content-header.download</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
